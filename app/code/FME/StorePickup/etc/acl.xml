<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">          
                <resource id="FME_Extensions::fmeextensions" title="FME Extensions">
                    <resource id="FME_StorePickup::main_menu" title="Google Map Stores Locator" 
                                          sortOrder="100">
                        <resource id="FME_StorePickup::manage_stores" title="Manage Stores" 
                                  sortOrder="22" />
                        <resource id="FME_StorePickup::manage_tags" title="Manage Tags"                               sortOrder="30" />
                        <resource id="FME_StorePickup::manage_holiday" title="Manage Holidays" 
                                  sortOrder="2" />
                        <resource id="FME_StorePickup::manage_timetable" title="Manage Timetable" 
                                  sortOrder="70" />
                    </resource>
                </resource>            
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="FME_StorePickup::configuration" title="FME Store Pickup Configuration" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
